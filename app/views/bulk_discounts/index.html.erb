
<h1> Bulk Discounts </h1>

<ul>

  <% @bulk_discounts.each do |bulk_discount| %> 
 
  <section id ="bulk_show_link-<%= bulk_discount.id %>">
    <li><%= "Bulk Discounts: " %><%= link_to " #{bulk_discount.id}", "/bulk_discounts/#{bulk_discount.id}" %></li>  
    <li><%= bulk_discount.percentage_discount %></li>
    <li><%= bulk_discount.quantity_threshold %></li>
  <% end %>
  </section>
</ul>

